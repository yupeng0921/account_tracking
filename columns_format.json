    "sequence" : [
        "AccountId",
        "EMail",
        "CustomerName",
        "Segment",
        "AccountManager",
        "KickoffMeeting",
        "InternetTrafficRequired",
        "Exception",
        "ExceptionApproved",
        "IPRecord",
        "ICPRecord",
